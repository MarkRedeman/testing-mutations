{
    "name": "renamed/test-with-mutations",
    "description": "A proof of concept running a test suite with mutated source code",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Redeman",
            "email": "markredeman@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "nikic/php-parser": "^3.0.0-alpha",
        "symfony/process": "2.*",
        "whatthejeff/nyancat-scoreboard": "^1.1"
    },

    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "humbug/humbug": "dev-master",
        "phpunit/php-code-coverage": "^4.0"
    },

    "autoload": {
        "psr-4": {
            "Renamed\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Renamed\\Tests\\": "tests"
        }
    },

    "bin": [
        "bin/white-wolf"
    ]
}
